{
  "kind": "collectionType",
  "collectionName": "change_logs",
  "info": {
    "singularName": "change-log",
    "pluralName": "change-logs",
    "displayName": "Change log",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "magnitude": {
      "type": "enumeration",
      "enum": [
        "minor",
        "major"
      ],
      "default": "minor"
    },
    "exposed": {
      "type": "boolean",
      "default": false
    }
  }
}
